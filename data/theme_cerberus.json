{
    "html, div#setting_window=>background-color": "#131313",
    "html, a=>color": "#f96503",
    "div.tab=>background-color": "#131313",
    "label, span, p=>color": "#f96503",
    "#file_naming label=>color": "#B9B9B9",
    "button=>border-color": "#7a807b",
    "button=>color": "#ccc",
    "button=>background-color": "#2b2b2b",
    "button.selected=>color": "#131313",
    "button.selected=>background-color": "#f96503",
    "button+selected:hover=>background-color": ["#f96503", "#222629"],
    "button+selected:hover=>color": ["#131313", "#ccc"],
    "button.tablinks=>color": "#ccc",
    "button.tablinks.selected=>color": "#131313",
    "button.tablinks:hover=>color": ["#131313", "#ccc"],
    "button.tablinks.selected=>background-color": "#f96503",
    "button.tablinks:hover=>background-color": ["#f96503", "#2b2b2b"],
    "button.tablinks:click=>color": ["#131313", "#ccc"],
    "button.tablinks:click=>background-color": ["#f96503", "#2b2b2b"],
    "button.tablinks+selected:hover=>background-color": ["#f96503", "#2b2b2b"],
    "button.tablinks+selected:hover=>color": ["#131313", "#ccc"],
    "button.postfix, button.safety_issue, button.game_mode, button.issue_type_1, button.issue_type_2:click=>background-color": ["#f96503", "#2b2b2b"],
    "button.postfix, button.safety_issue, button.game_mode, button.issue_type_1, button.issue_type_2:click=>color": ["#131313", "#ccc"],
    "input[type='checkbox']:checked=>--after-color": "#f96503"
}