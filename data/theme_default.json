{
    "html, div#setting_window=>background-color": "#fff",
    "html, a=>color": "#494949",
    "div.tab=>background-color": "#ddd",
    "label, span, p=>color": "#494949",
    "#file_naming label=>color": "#B9B9B9",
    "button=>border-color": "#606060",
    "button=>color": "#494949",
    "button=>background-color": "#f1f1f1",
    "button.selected=>color": "#494949",
    "button.selected=>background-color": "#acacac",
    "button+selected:hover=>background-color": ["#acacac", "#f1f1f1"],
    "button+selected:hover=>color": ["#494949", "#494949"],
    "button.tablinks=>color": "#494949",
    "button.tablinks:hover=>color": ["#494949", "#494949"],
    "button.tablinks.selected=>background-color": "#ccc",
    "button.tablinks:hover=>background-color": ["#ccc", "#f1f1f1"],
    "button.tablinks:click=>color": ["#494949", "#494949"],
    "button.tablinks:click=>background-color": ["#ccc", "#f1f1f1"],
    "button.tablinks+selected:hover=>background-color": ["#ccc", "#f1f1f1"],
    "button.postfix, button.safety_issue, button.game_mode, button.issue_type_1, button.issue_type_2:click=>background-color": ["#acacac", "#f1f1f1"],
    "button.postfix, button.safety_issue, button.game_mode, button.issue_type_1, button.issue_type_2:click=>color": ["#494949", "#494949"],
    "input[type='checkbox']:checked=>--after-color": "#000"
}