{
    "html=>background-color": "#fff",
    "html, a=>color": "#000",
    "div.tab=>background-color": "#f1f1f1",
    "div.tab=>border-color": "#ccc",
    "div.tabcontent=>border-color": "#ccc",
    "label, span, p=>color": "#000",
    "button=>border-color": "#ddd",
    "button=>color": "#000",
    "button=>background-color": "#f1f1f1",
    "button.selected=>color": "#000",
    "button.selected=>background-color": "#acacac",
    "button+selected:hover=>background-color": ["#acacac", "#f1f1f1"],
    "button+selected:hover=>color": ["#000", "#000"],
    "button.tablinks=>color": "#000",
    "button.tablinks:hover=>color": ["#000", "#000"],
    "button.tablinks.active=>background-color": "#ccc",
    "button.tablinks:hover=>background-color": ["#ccc", "#f1f1f1"],
    "button.tablinks:click=>color": ["#000", "#000"],
    "button.tablinks:click=>background-color": ["#ccc", "#f1f1f1"],
    "button.tablinks+active:hover=>background-color": ["#ccc", "#f1f1f1"],
    "button.postfix, button.issue_type_2:click=>background-color": ["#acacac", "#f1f1f1"]
}