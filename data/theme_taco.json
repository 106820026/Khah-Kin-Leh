{
    "html=>background-color": "#222629",
    "html, a=>color": "#b3bbb5",
    "div.tab=>background-color": "#222629",
    "div.tab=>border-color": "#ccc",
    "div.tabcontent=>border-color": "#ccc",
    "label, span, p=>color": "#85c136",
    "button=>border-color": "#7a807b",
    "button=>color": "#7a807b",
    "button=>background-color": "#222629",
    "button.selected=>color": "#222629",
    "button.selected=>background-color": "#85c136",
    "button+selected:hover=>background-color": ["#85c136", "#222629"],
    "button+selected:hover=>color": ["#222629", "#7a807b"],
    "button.tablinks=>color": "#ccc",
    "button.tablinks:hover=>color": ["#ccc", "#ccc"],
    "button.tablinks.active=>background-color": "#393d40",
    "button.tablinks:hover=>background-color": ["#393d40", "#222629"],
    "button.tablinks:click=>color": ["#ccc", "#ccc"],
    "button.tablinks:click=>background-color": ["#393d40", "#222629"],
    "button.tablinks+active:hover=>background-color": ["#393d40", "#222629"],
    "button.postfix, button.issue_type_2:click=>background-color": ["#85c136", "#222629"],
    "button.postfix, button.issue_type_2:click=>color": ["#222629", "#7a807b"]
}